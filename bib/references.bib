@article{sensorsstudy,
author = {Ming Liu},
title = {A Study of Mobile Sensing Using Smartphones},
journal = {International Journal of Distributed Sensor Networks},
volume = {9},
number = {3},
pages = {272916},
year = {2013},
doi = {10.1155/2013/272916},

URL = { 
        http://dx.doi.org/10.1155/2013/272916
    
},
eprint = { 
        http://dx.doi.org/10.1155/2013/272916
    
}
,
    abstract = { Traditional mobile sensing-based applications use extra equipments which are unrealistic for most users. Smartphones develop in a rapid speed in recent years, and they are becoming indispensable carry-on of daily life. The sensors embedded in them provide various possibilities for mobile applications, and these applications are helping and changing the way of our living. In this paper, we analyze and discuss existing mobile applications; after that, future directions are pointed out. }
}

